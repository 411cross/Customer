<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.peek_mapdemotest.nurseapp.Activity.HomeActivity">

    <ImageButton
        android:id="@+id/imageButton"
        android:layout_width="66dp"
        android:layout_height="74dp"
        app:srcCompat="@mipmap/ic_launcher"
        tools:layout_editor_absoluteX="0dp"
        tools:layout_editor_absoluteY="-3dp" />

    <ImageView
        android:id="@+id/imageView9"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:scaleType="fitXY"
        app:srcCompat="@mipmap/background"
        android:layout_below="@+id/imageButton"
        />
    <TextView
        android:id="@+id/textView21"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView9"
        android:layout_marginTop="10dp"
        android:textColor="#ee3a8c"
        android:textStyle="bold"
        android:textSize="25sp"
        android:layout_centerHorizontal="true"
        android:text="精挑细选"
        />
    <Button
        android:id="@+id/NurseListBt"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="开 始"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:background="#00b0ff"
        android:layout_below="@+id/textView21"
        android:layout_centerHorizontal="true"
        android:layout_marginTop="10dp"
        android:layout_marginBottom="10dp"
        />
    <ImageView
        android:id="@+id/imageView10"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:scaleType="fitXY"
        app:srcCompat="@mipmap/background"
        android:layout_below="@+id/NurseListBt"

        />

    <TextView
        android:id="@+id/textView20"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/imageView10"
        android:layout_marginTop="10dp"
        android:text="省心预约"
        android:textStyle="bold"
        android:layout_centerHorizontal="true"
        android:textColor="#b03060"
        android:textSize="25sp"
        />
    <Button
        android:id="@+id/button3"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="内 科"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:background="#00b0ff"
        android:layout_marginTop="10dp"
        android:layout_toLeftOf="@+id/button4"
        android:layout_marginRight="10dp"
        android:layout_below="@+id/textView20"
        />

    <Button
        android:id="@+id/button4"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:background="#00b0ff"
        android:text="外 科"

        android:layout_alignBaseline="@+id/button3"
        android:layout_alignBottom="@+id/button3"
        android:layout_alignLeft="@+id/NurseListBt"
        android:layout_alignStart="@+id/NurseListBt"
        />
    <Button
        android:id="@+id/button5"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="妇 科"
        android:textSize="20dp"
        android:textColor="#ffffff"
        android:background="#00b0ff"
        android:layout_alignBottom="@+id/button4"
        android:layout_toRightOf="@+id/button4"
        android:layout_marginLeft="10dp"
        />

    <Button
        android:id="@+id/standard_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_below="@+id/button3"
        android:layout_marginEnd="26dp"
        android:layout_marginRight="26dp"
        android:layout_marginTop="13dp"
        android:layout_toLeftOf="@+id/button6"
        android:layout_toStartOf="@+id/button6"
        android:background="#00b0ff"
        android:text="标 准"
        android:textColor="#ffffff"
        android:textSize="20dp" />

    <Button
        android:id="@+id/serious_btn"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_alignBaseline="@+id/standard_btn"
        android:layout_alignBottom="@+id/standard_btn"
        android:layout_toEndOf="@+id/standard_btn"
        android:layout_toRightOf="@+id/standard_btn"
        android:background="#00b0ff"
        android:text="重 症"
        android:textColor="#ffffff"
        android:textSize="20dp" />

    <TextView
        android:id="@+id/textView"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="用户编号"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/imageButton"
        android:layout_marginTop="28dp"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <Button
        android:id="@+id/button6"
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:text="  添加病人   "
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:background="#00b0ff"

        android:layout_marginRight="12dp"
        android:layout_marginEnd="12dp"
        android:layout_above="@+id/imageView12"
        android:layout_alignRight="@+id/imageView11"
        android:layout_alignEnd="@+id/imageView11"
        android:layout_marginBottom="12dp" />


    <ImageView
        android:id="@+id/imageView12"
        android:layout_width="match_parent"
        android:layout_height="5dp"
        android:scaleType="fitXY"
        app:srcCompat="@mipmap/background"
        android:layout_above="@+id/imageView11"
        android:layout_marginBottom="10dp"
        />
    <ImageView

        android:id="@+id/imageView11"
        android:layout_width="280sp"
        android:layout_height="51sp"
        android:layout_marginBottom="10dp"
        android:layout_centerHorizontal="true"
        android:layout_alignParentBottom="true"
        app:srcCompat="@mipmap/button1"
        />
    <Button
        android:layout_width="wrap_content"
        android:layout_height="50dp"
        android:id="@+id/CheckOrder"
        android:text="    查询订单  "
        android:textColor="#ffffff"
        android:textSize="20dp"
        android:background="#00b0ff"

        android:layout_alignBaseline="@+id/button6"
        android:layout_alignBottom="@+id/button6"
        android:layout_toRightOf="@+id/imageButton"
        android:layout_toEndOf="@+id/imageButton" />
</RelativeLayout>
