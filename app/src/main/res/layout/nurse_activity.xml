<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent">
    <TextView
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:gravity="center"
        android:id="@+id/tipsTv"
        android:text=""/>




    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="36dp"
            android:id="@+id/linearLayout5">
            <Spinner
                android:layout_width="0dp"
                android:layout_height="match_parent"
                android:layout_weight="1"
                android:id="@+id/Spinner1"
                android:entries="@array/protectArea"
                android:paddingTop="0dp"
                android:paddingBottom="0dp"


            ></Spinner>

        <Spinner
            android:id="@+id/Spinner2"

            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:entries="@array/protectYear"
            android:paddingBottom="0dp"
            android:paddingTop="0dp"
            android:background="@drawable/shape_drop_down_normal"
            android:dropDownSelector="@color/color_white"

            >

        </Spinner>

        <Spinner
            android:id="@+id/Spinner3"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:entries="@array/sex"
            android:paddingBottom="0dp"
            android:paddingTop="0dp"
            android:background="@drawable/shape_drop_down_normal"
            android:dropDownSelector="@color/color_white">

        </Spinner>

        <Spinner
            android:id="@+id/Spinner4"
            android:layout_width="0dp"
            android:layout_height="match_parent"
            android:layout_weight="1"
            android:entries="@array/evaluation"
            android:paddingBottom="0dp"
            android:paddingTop="0dp"
            android:background="@drawable/shape_drop_down_normal"
            android:dropDownSelector="@color/color_white"
            ></Spinner>


    </LinearLayout>

        <ListView
            android:id="@+id/NurseListview"
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_below="@+id/linearLayout5"></ListView>

    </RelativeLayout>

</FrameLayout>